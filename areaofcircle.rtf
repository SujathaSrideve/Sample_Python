def areaofcircle (radius):
    Area = 3.14 * radius * radius
    return Area

def volumeofcyclinder(area,height):
  Vol = area * height
  return Vol


rad_cal = int(input("Enter radius of circle :"))
height_calc = int(input("Enter height of circle :"))

area_calc = areaofcircle(rad_cal)
vol_calc = volumeofcyclinder(area_calc,height_calc)

print("Area is : ", area_calc)
print("Volume of Cylinder : ", vol_calc)
